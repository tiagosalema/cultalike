type User {
  id: ID! @unique
  name: String!
  ratedMovies: [RatedMovie]!
    @relation(name: "UserRatedMovie", onDelete: CASCADE)
}

type Movie {
  id: ID! @unique
  title: String!
  ratedMovie: [RatedMovie]! @relation(name: "RatedMovie", onDelete: CASCADE)
}

type RatedMovie {
  id: ID! @unique
  movie: Movie! @relation(name: "RatedMovie", onDelete: SET_NULL)
  rate: Int!
  rater: User! @relation(name: "UserRatedMovie", onDelete: SET_NULL)
}
